<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <Title>Passkeys Preview</Title>
    <Product>Passkeys</Product>
    <Version>0.2.7</Version>
    <Company>ChatAIze</Company>
    <Authors>Marcel Kwiatkowski</Authors>
    <Copyright>Â© ChatAIze 2024</Copyright>
    <AssemblyName>ChatAIze.Passkeys.Preview</AssemblyName>
    <RootNamespace>ChatAIze.Passkeys.Preview</RootNamespace>
    <PackageId>ChatAIze.Passkeys.Preview</PackageId>
    <PackageIcon>icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://www.chataize.com</PackageProjectUrl>
